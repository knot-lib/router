  "keywords": ["PHP","Router"],
  "homepage": "https://github.com/knot-lib/router",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Katsuki Shuto",
      "email": "stk2k@knot-framework.com"
    }
  ],
  "require": {
