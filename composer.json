    "stk2k/util": "~0.1",
    "stk2k/file": "~0.1",
    "knot-lib/exception": "~0.1",
    "knot-lib/config": "~0.1",
    "knot-lib/cache": "~0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3.0",
    "php-coveralls/php-coveralls": "^2.0"
